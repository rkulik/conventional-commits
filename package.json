  "dependencies": {
    "@inquirer/prompts": "8.0.1",
    "chalk": "5.6.2",
    "clipboardy": "5.0.1",
    "figlet": "1.9.4",
    "shell-quote": "1.8.3"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@jest/globals": "30.2.0",
    "@rkulik/eslint-config": "1.7.0",
    "@rkulik/prettier-config": "1.1.0",
    "@types/figlet": "1.7.0",
    "@types/node": "24.10.1",
    "@types/shell-quote": "1.7.5",
    "eslint": "9.39.1",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "mock-stdin": "1.0.0",
    "ts-jest": "29.4.5",
    "ts-jest-resolver": "2.0.1",
    "tsup": "8.5.1",
    "typescript": "5.9.3"
  },
  "prettier": "@rkulik/prettier-config"
